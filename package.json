  "devDependencies": {
    "ava": "^5.2.0",
    "c8": "^7.11.0",
    "eslint": "^8.12.0",
    "mockdate": "^3.0.5"
  },
  "dependencies": {
    "fdir": "^6.0.1",
    "gray-matter": "^4.0.3",
    "jsonpath": "^1.1.1",
    "meow": "^11.0.0"
  }
}
