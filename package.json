{
  "name": "data-extractor",
  "type": "module",
  "version": "0.0.0",
  "description": "Parses content files to generate a dataset.",
  "keywords": [
    "cloudcannon"
  ],
  "homepage": "https://github.com/CloudCannon/data-extractor#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/CloudCannon/data-extractor.git"
  },
  "bugs": {
    "url": "https://github.com/CloudCannon/data-extractor/issues",
    "email": "support@cloudcannon.com"
  },
  "main": "src/index.js",
  "scripts": {
    "test": "ava",
    "test:watch": "ava --watch --verbose",
    "test:coverage": "c8 --all ava",
